{
  "version": 2,
  "name": "checkout-syncpay",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "SYNCPAY_CLIENT_ID": "708ddc0b-357d-4548-b158-615684caa616",
    "SYNCPAY_CLIENT_SECRET": "c08d40e5-3049-48c9-85c0-fd3cc6ca502c"
  }
}
